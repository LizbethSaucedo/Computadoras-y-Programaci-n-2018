#include<stdio.h>
 int main(int argc, char *argv[])
 {
 	int v1=8;
 	int v2=5;
 	int resultado=0;
 	
 //suma	
 	resultado=v1 + v2;
 	printf("%d+%d=%d\n",v1,v2,resultado);
 	
 //resta	
 	resultado=var1 - var2;
 	printf("%d-%d=%d\n",v1,v2,resultado);
 	
 //multiplicaci√≥n	
 	resultado=v1 * v2;
 	printf("%d*%d=%d\n",v1,v2,resultado);
 	
 //division	
 	resultado=v1 / v2;
 	printf("%d/%d=%d\n",v1,v2,resultado);
 	
 //modulo
    resultado=v1 % v2;
	printf("%dmod%d=%d\n",v1,v2,resultado);
	
	return 0;
	
		
 }
